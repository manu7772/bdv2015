<!DOCTYPE html> <!-- HTML 5 -->
<html lang="fr">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<title>{% block title %}Labo{% endblock %}</title>
		<link rel="icon" type="image/x-icon" href="{{ asset(app.session.get('version').favicon) }}" />
		{% stylesheets filter='cssrewrite, lessphp, ?yui_css'
		 %}
		<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
		{% endstylesheets %}
	</head>
	<body>
		{% block navbar %}
			{{ render(controller("AcmeGroupLaboBundle:Labo:navbar")) }}
		{% endblock navbar %}
		<!-- DEBUT : CONTENU DE LA PAGE -->
		{% block body_container %}
		{% endblock body_container %}
		<!-- FIN : CONTENU DE LA PAGE -->
		{% include ":common:iframe.html.twig" %}
		{% include ":common:devStuffs.html.twig" %}
		{% include ":common:hiddenStuffs.html.twig" %}
		{% javascripts filter='?yui_js'
		 %}
		<script type="text/javascript" src="{{ asset_url }}"></script>
		{% endjavascripts %}
		{% block footer %}
		{% endblock %}
	</body>
</html>